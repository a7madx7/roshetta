    <div class="card section scrollspy" id="inputs-demo">
      <div class="card-content"><span class="card-title">Prescription meta data</span>
        <div class="row">
          <%= form_with(model: rx, local: true, html: {class: 'col s12'}) do |form| %>
            <% if rx.errors.any? %>
              <div id="error_explanation">
                <h2><%= pluralize(rx.errors.count, "error") %> prohibited this rx from being saved:</h2>

                <ul>
                  <% rx.errors.full_messages.each do |message| %>
                    <li><%= message %></li>
                  <% end %>
                </ul>
              </div>
            <% end %>

            <div class="row">
              <%= form.fields_for :patient do |p| %>
                <div class="input-field col s6">
                  <%= p.label :name, class: 'active', for: 'rx_patient_name' %>
                  <%= p.text_field :name,   class: 'validate', placeholder: 'Patient full name goes here' %>
                </div>
              <% end %>


              <div class="input-field col s6">
                <%= form.label :allowed_dispense_count, for: 'rx_allowed_dispense_count' %>
                <%= form.number_field :allowed_dispense_count, id: :rx_allowed_dispense_count, class: 'validate' %>
              </div>

            </div>

            <div class="row">
              <div class="file-field input-field col s12">
                <%= form.label :picture %>
                <%= form.file_field :picture, id: :rx_picture %>
              </div>
            </div>
            <br />
            <div class="row">
              <div class="input-field col-s12">
                <%= form.text_area :professional_comment, as: :text, id: :rx_professional_comment, placeholder: 'Write your professional comment on the case here.' %>
              </div>
            </div>
            <div class="row">
              <div class="-input-field col s12">
                <%= form.text_field :short_diagnosis, id: :rx_short_diagnosis, class: 'validate', placeholder: 'Write short diagnosis' %>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <%= form.text_area :full_diagnosis, as: :text, id: :rx_full_diagnosis, placeholder: 'Write full diagnosis with extra comments here', class: 'validate' %>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <%= form.label :starts_at %>
                <%= form.datetime_select :starts_at, id: :rx_starts_at %>
